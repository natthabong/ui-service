<div th:fragment="content"
	sec:authorize="hasAnyAuthority('VIEW_ALL_ORGANIZE_FILE_LAYOUTS','MANAGE_ALL_ORGANIZE_FILE_LAYOUTS')">
	<div id="wrapper"
		ng-controller="FileLayoutListController as fileLayoutsCtrl">
		<input type="hidden" ng-init="fileLayoutsCtrl.viewAllConfig=true"
			sec:authorize="hasAuthority('VIEW_ALL_ORGANIZE_FILE_LAYOUTS')" /> <input
			type="hidden" ng-init="fileLayoutsCtrl.manageAllConfig=true"
			sec:authorize="hasAuthority('MANAGE_ALL_ORGANIZE_FILE_LAYOUTS')" />
		<div class="panel panel-default config-box">
			<div class="panel-body">
				<div class="row">
					<div class="col-lg-10 col-md-10 col-xs-10">
						<h3 class="animated fadeInLeft" id="file-layouts-header-label">{{'Import
							file layout' | translate}}</h3>
					</div>
					<div class="col-lg-2 col-md-2 col-xs-2"
						ng-init="fileLayoutsCtrl.init('AP_DOCUMENT')">
						<br />
						<scf-button
							id="layout-{{fileLayoutsCtrl.data.displayName}}-setup-button"
							class="btn-default gec-btn-action"
							ng-click="fileLayoutsCtrl.newFileLayout(fileLayoutsCtrl.data)"
							title="Config a file layout"
							ng-disabled="fileLayoutsCtrl.unauthenConfig()"> <i
							class="fa fa-cog" aria-hidden="true"></i> </scf-button>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>